{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "open-interpreter",
  "projectId": "prj_VOcIl5dMZoWyjCda8pcXsdGCIrbc",
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHON_VERSION": "3.11",
    "FLASK_ENV": "production"
  }
}
